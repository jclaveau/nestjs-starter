{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/src/server/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test": "jest --collect-coverage --setupFiles dotenv-flow/config",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --setupFiles dotenv-flow/config",
    "console": "ts-node -r tsconfig-paths/register src/server/console.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "client:generate:schema": "zeus src/schema.gql src/client/app/types --ts"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@apollo/gateway": "^0.46.0",
    "@babel/core": "^7.0.0-0",
    "@nestjs/common": "^8.2.5",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.5",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/typeorm": "^8.0.3",
    "apollo-server-express": "^3.6.1",
    "axios": "^0.24.0",
    "class-transformer": "0.5.0",
    "class-validator": "^0.13.0",
    "commander": "^8.3.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "google-auth-library": "^7.0.4",
    "graphql": "^15.0.0",
    "graphql-tools": "^8.2.0",
    "nestjs-console": "^7.0.1",
    "next": "^12.0.8",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "pg": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "ts-morph": "^13.0.2",
    "typeorm": "^0.2.31",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.9",
    "@types/passport-google-oauth20": "^2.0.7",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/passport-oauth2": "^1.4.10",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "5.5.3",
    "fishery": "^2.1.0",
    "graphql-zeus": "4.0.4",
    "jest": "^27.4.7",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
